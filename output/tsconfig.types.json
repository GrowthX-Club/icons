{
  "compilerOptions": {
    "target": "es2016",

    "jsx": "react-jsx",

    "module": "ESNext",

    "moduleResolution": "Node",

    "declaration": true,

    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true,

    "strict": true,

    "skipLibCheck": true,
    "outDir": "../dist/types",
    "emitDeclarationOnly": true
  }
}
