{
  "name": "@growthx-club/icons-builder",
  "version": "1.0.0",
  "description": "Package for storing all the icons used across the GrowthX Platform",
  "main": "index.js",
  "type": "module",
  "repository": {
    "url": "https://github.com/GrowthX-Club/icons"
  },
  "scripts": {
    "convert-svg": "svgr src --out-dir output --typescript --jsx-runtime automatic --template template.cjs",
    "build": "tsc --project output --outDir dist"
  },
  "keywords": [
    "icons"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "typescript": "^5.4.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "proseWrap": "always",
    "singleQuote": true
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "@types/react": "^18.2.64"
  }
}
